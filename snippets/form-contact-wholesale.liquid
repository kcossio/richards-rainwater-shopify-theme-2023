
<header class="contact-form-header">
<h2 class="form-title">Wholesaler Application</h2>
<p>Interested in selling Richard’s in your store or restaurant? Great! Fill out this simple form and somebody from our team will get follow up within 2 business days.</p>
</header><!--End contact-form-header-->

{%- form 'contact', id: 'ContactFormWholesaleInquiries', class: 'form-contact-form' -%}
    {%- if form.posted_successfully? -%}
      <h2 class="form-status form-status-list form__message" tabindex="-1" autofocus>
        {% render 'icon-success' %}
        {{ 'templates.contact.form.post_success' | t }}
      </h2>
    {%- elsif form.errors -%}
      <div class="form__message">
        <h2 class="form-status caption-large text-body" role="alert" tabindex="-1" autofocus>
          {% render 'icon-error' %}
          {{ 'templates.contact.form.error_heading' | t }}
        </h2>
      </div>
      <ul class="form-status-list caption-large" role="list">
        <li>
          <a href="#ContactForm-email" class="link">
            {{ form.errors.translated_fields.email | capitalize }}
            {{ form.errors.messages.email }}
          </a>
        </li>
      </ul>
    {%- endif -%}

    <div class="field full">        
        <input type="date" name="contact[date]" class="field__input" id="date" placeholder="Date"/>
        <label class="label" for="date">Date</label>
    </div><!--End field-->

    <div class="field full">
        <input class="field__input required"
            type="text"
            id="WSForm-TaxID"
            name="contact[federal_tax_id]" 
            required
            placeholder="Federal Tax ID"
            oninvalid="this.setCustomValidity('Please enter your federal tax ID')"
        >
            <label class="label" for="WSForm-TaxID">Federal Tax ID <span class="required">*</span></label>
      </div><!--End field-->

      <div class="field full">        
        <input type="text" name="contact[company_name]" class="field__input required" required id="company-name" placeholder="Company Name" oninvalid="this.setCustomValidity('Please enter your company name')"/>
        <label class="label" for="company-name">Company Name<span class="required">*</span></label>
    </div><!--End field-->

    <div class="contact__fields">

      <div class="field">        
          <input type="text" name="contact[buyer_firstname]" class="field__input required" required id="buyer-firstname" placeholder="Buyer Contact: First Name" oninvalid="this.setCustomValidity('Please enter your first name')"/>
          <label class="label" for="buyer-firstname">Buyer Contact: First Name <span class="required">*</span></label>
      </div><!--End field-->

      <div class="field">        
          <input type="text" name="contact[buyer_lastname]" class="field__input required " required id="buyer-lastname" placeholder="Buyer Contact: Last Name" oninvalid="this.setCustomValidity('Please enter your last name')"/>
          <label class="label" for="buyer-lastname">Buyer Contact: Last Name <span class="required">*</span></label>
      </div><!--End field-->

    </div><!--End contact__fields-->

    <div class="contact__fields">

      <div class="field">
        <input
          type="tel"
          id="ContactForm-phone"
          class="field__input"
          autocomplete="tel"
          name="contact[{{ 'templates.contact.form.phone' | t }}]"
          pattern="[0-9\-]*"
          required 
          value="{% if form.phone %}{{ form.phone }}{% elsif customer %}{{ customer.phone }}{% endif %}"
          placeholder="{{ 'templates.contact.form.phone' | t }}"
        >
        <label class="label" for="ContactForm-phone">Phone Number <span class="required">*</span></label>
      </div>

      <div class="field">        
          <input type="tel" pattern="[0-9\-]*" name="contact[fax_number]" class="field__input" id="fax-number" placeholder="Fax Number"/>
          <label class="label" for="fax-number">Fax Number </label>
      </div><!--End field-->

    </div><!--End contact__fields-->

    <div class="form-section">

      <div class="field">
        <textarea
          rows="3"
          id="ContactForm-body"
          class="text-area field__input"
          name="contact[message]"
          placeholder="Message"
        >
          {{- form.body -}}
        </textarea>
        <label class="form__label label" for="ContactForm-body">Message / Additional Info</label>
      </div><!--End field-->

    </div><!--End form-section-->

    <div class="form_section">
      <h2>Address <span class="required red">*</span></h2>
    
      <div class="field under full">
